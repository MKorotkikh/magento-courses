<?xml version="1.0" ?>
<config>
    <modules>
        <Ainstainer_TechTalk>
            <version>0.0.1</version>
        </Ainstainer_TechTalk>
    </modules>
    <frontend>
        <routers>
            <techtalk>
                <use>standard</use>
                <args>
                    <module>Ainstainer_TechTalk</module>
                    <frontName>techtalk</frontName>
                </args>
            </techtalk>
        </routers>
        <layout>
            <updates>
                <techtalk module="TechTalk">
                    <file>techtalk.xml</file>
                </techtalk>
            </updates>
        </layout>
    </frontend>
    <global>
        <models>
            <techtalk>
                <class>Ainstainer_TechTalk_Model</class>
                <resourceModel>techtalk_resource</resourceModel>
            </techtalk>
            <techtalk_resource>
                <class>Ainstainer_TechTalk_Model_Resource</class>
                <entities>
                    <contact>
                        <table>ain_contact</table>
                    </contact>
                </entities>
            </techtalk_resource>
        </models>
        <resources>
            <techtalk_setup>
                <setup>
                    <module>Ainstainer_TechTalk</module>
                </setup>
            </techtalk_setup>
        </resources>
        <blocks>
            <techtalk>
                <class>Ainstainer_TechTalk_Block</class>
            </techtalk>
        </blocks>
    </global>
</config>